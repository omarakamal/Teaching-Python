class vehichle():
    def __init__(self, color, price, gas):
        self.color = color
        self.price= price
        self.gas = gas

    def Filluptank(self):
        self.gas = 100
    def Emptytank(self):
        self.gas = 0
    def Showtank(self):
        return self.gas
        print(self.gas)

class car(vehichle):
    def __init__(self, color, price, gas, speed):
        super().__init__(color, price, gas)
        self.speed = speed

    def beep(self):
        print("beep beep")


class truck(vehichle):
    def __init__(self, color, price, gas, wheels):
        super().__init__(color, price, gas)
        self.wheels = wheels

    def beep(self):
        print("honk honk")

chevy = car("black",50000, 50, 240)
chevy.beep()

eighteenwheeler = truck("red", 80000, 99, 18)
eighteenwheeler.Filluptank()
#chevy.Showtank()
print(chevy.gas)
print(chevy.speed)
#print(chevy.price)

